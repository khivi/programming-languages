{
  "private": true,
  "dependencies": {
    "fs": "0.0.1-security",
    "readline": "^1.3.0",
    "test": "^0.6.0"
  },
  "devDependencies": {
    "ava": "*",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-ava": "^10.3.0",
    "nyc": "*"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "ava --watch",
    "coverage": "nyc ava",
    "lint": "eslint --ext .js --ignore-pattern .gitignore ."
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "extends": [
      "google",
      "plugin:ava/recommended"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "require-jsdoc": "off",
      "spaced-comment": "off"
    }
  }
}
