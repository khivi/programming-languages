{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": [
      "react",
      "@typescript-eslint",
      "jest"
  ],
  "env": {
    "browser": true,
    "jasmine": true,
    "jest": true,
     "es6": true
  },
  "rules": {
  },
  "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  },
  "parser": "@typescript-eslint/parser"
}

